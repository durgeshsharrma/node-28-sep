<% layout('/layouts/boilerplate') -%>


<div class="row">
    <h4 class="text-center mt-3">Users List</h4>
    <table class="table text-center mt-3">
        <thread>
            <tr>
                 <th>Username</th>
                 <th>First Name</th>
                 <th>Last Name</th>
                 <th>Country Code</th>
                 <th>Mobile Number</th>
                 <th>Password</th>
                 
            </tr>
            <% for(stud of student) { %>
             <tr>
                <td><%= stud.username  %></td>
                <td><%= stud.first_name  %></td>
                <td><%= stud.last_name %></td>
                <td><%= stud.country_code %></td>
                <td><%= stud.phoneNo  %></td>
                <td><%= stud.password %></td>
                
            </tr>

          <% } %> 
        </thread>
    </table>
</div>


